{
  "name": "electron-startup",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "antd": "^3.26.4",
    "codemirror": "^5.50.0",
    "electron-is-dev": "^1.1.0",
    "hypermd": "^0.3.11",
    "marked": "^0.8.0",
    "moment": "latest",
    "react": "^16.12.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build ",
    "test": "react-app-rewired test --env=jsdom",
    "build:copy": "react-app-rewired build && npm run copy:electron",
    "copy:electron": "cp -r ./app/. ./build",
    "pack:mac": "electron-builder --mac",
    "pack:win": "electron-builder --win --x64",
    "build:mac": "npm run build:copy && npm run pack:mac",
    "build:win": "npm run build:copy && npm run pack:win"
  },
  "build": {
    "productName": "electron-startup",
    "appId": "com.xxx.xxxxx",
    "copyright":"xxxx",
    "files": [
      "build/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": ["dmg","zip"]
    },
    "win": {
      "icon": "xxx/icon.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "extraMetadata": {
      "main": "build/main.js"
    }
  },
  "homepage": ".",
  "devDependencies": {
    "less-loader": "^5.0.0",
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.2.13",
    "electron-builder": "^22.3.2",
    "less": "^3.10.3",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^2.1.3",
    "electron": "^7.1.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
